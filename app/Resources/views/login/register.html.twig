{% extends 'base_login.html.twig' %}

{% block body %}

    {{ form_start(form) }}
    {{ form_row(form.username) }}
    {{ form_row(form.email) }}
    {{ form_row(form.plainPassword.first) }}
    {{ form_row(form.plainPassword.second) }}

    <button type="submit">Register!</button>
    {{ form_end(form) }}

{% endblock %}

{% block description %}
    Erstelle deinen eigenen Account und sei Teil der Wheel Of Fortune Community!
{% endblock %}
